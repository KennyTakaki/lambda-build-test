{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "typecheck": {
      "outputs": []
    },
    "lint:md": {
      "outputs": []
    },
    "format":{
      "outputs": []
    }
  }
}
